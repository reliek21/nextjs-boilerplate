{
	"$schema": "https://json.schemastore.org/eslintrc",
	"root": true,
	"extends": [
		"next",
		"next/core-web-vitals",
		"prettier",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"plugins": ["prettier"],
	"rules": {
		"no-unused-vars": "off",
		"object-curly-spacing": ["error", "always"],
		"prettier/prettier": "error"
	}
}
